{
  "name": "saggre/phpdocumentor-markdown",
  "description": "Markdown template for phpDocumentor3",
  "authors": [
    {
      "name": "Saggre",
      "email": "sakri.koskimies@hotmail.com"
    }
  ],
  "version": "0.1.4",
  "type": "library",
  "keywords": [
    "phpdocumentor",
    "phpdoc",
    "markdown",
    "github",
    "gitlab",
    "documentation"
  ],
  "require-dev": {
    "php": ">=7.4",
    "ext-json": "*",
    "phpunit/phpunit": "^9.5",
    "twig/twig": "2.*",
    "phpdocumentor/phpdocumentor": "^3",
    "symfony/filesystem": "^5.4"
  },
  "autoload": {
    "psr-4": {
      "PhpDocumentorMarkdown\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PhpDocumentorMarkdown\\Test\\Unit\\": "tests/unit",
      "PhpDocumentorMarkdown\\Test\\Integration\\": "tests/integration",
      "PhpDocumentorMarkdown\\Test\\Util\\": "tests/util"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "create-docs": "phpdoc && cp -r example/* docs/.wiki"
  },
  "license": "MIT",
  "config": {
    "allow-plugins": {
      "symfony/flex": false
    }
  }
}
